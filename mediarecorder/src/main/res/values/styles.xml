<resources>

   

    <!-- 水平分割线 -->
    <style name="HorizontalDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.5px</item>
        <item name="android:background">@color/bg_gray</item>
    </style>

    <!-- 垂直分割线 -->
    <style name="VerticalDivider">
        <item name="android:layout_width">1.5px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/divider_gray</item>
    </style>

    <style name="ToggleButtonText">
        <item name="android:textOn">On</item>
        <item name="android:textOff">Off</item>
    </style>
</resources>
